<%
locals.OLSKPageTitle = OLSKLocalized('MSWVitrineTitle');
locals.OLSKPageDescription = OLSKLocalized('MSWVitrineDescription');
locals.OLSKPageAssets = [
	'/_shared/__external/normalize.css/normalize.css',
	'/_shared/__external/OLSKLayout/ui-style.css',
	'/_shared/common/ui-style.css',
	'ui-style.css',
];
locals.OLSKPageUseLivereload = process.env.NODE_ENV === 'development';
locals.OLSKPagePublicConstants = {
	OLSKSharedPageCurrentLanguage: OLSKSharedPageCurrentLanguage,
};
%>

<div class="MSWVitrine OLSKCommon OLSKCommonCapped">

<%- include('../_shared/__external/OLSKLanguageSwitcher/main.ejs') %>

<hr role="presentation" />

<div class="MSWVitrineIdentity OLSKCommonCard OLSKCommonIdentityCard">
	<img class="MSWVitrineIdentityLogo" src="/_shared/MSWRootLink/ui-assets/identity.svg" role="presentation" />
	<section>
		<span class="MSWVitrineIdentityName"><%= OLSKLocalized('MSWVitrineTitle') %></span>
		<span class="MSWVitrineIdentityBlurb"><%= OLSKLocalized('MSWVitrineDescription') %></span>
	</section>
</div>

<p class="OLSKCommonWhatIsIt"><%= OLSKLocalized('OLSKCommonWhatIsItText') %></p>

<article class="MSWVitrineContent">

<%- OLSKStringReplaceTokens(MSWVitrineContent, {}) %>

</article>

<hr role="presentation" />

<%- include('../_shared/__external/ROCORootLink/main.ejs') %>

</div>
